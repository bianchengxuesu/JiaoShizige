<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ischoolbar.programmer.dao.admin.ZQiKanDao">
	<!-- 贡献插入操作 -->
	<insert id="add" parameterType="com.ischoolbar.programmer.entity.admin.Qikanwenzhang">
		insert into qikanwenzhang(pername,dename,denames,description,path) values(null,#{dename},#{denames},#{description},#{path})
	</insert>
	<!-- 获取所有期刊信息 -->
	<select id="findAll" resultType="com.ischoolbar.programmer.entity.admin.Qikanwenzhang">
		select * from  qikanwenzhang
	</select>
	<!-- id查询期刊信息 -->
	<select id="queryById" resultType="com.ischoolbar.programmer.entity.admin.Qikanwenzhang">
		select * from qikanwenzhang where pername=#{pername}
	</select>
</mapper>